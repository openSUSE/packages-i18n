msgstr "SLF4J JDK14 バインディング。"

#. leap/slf4j/slf4j-log4j12/summary
msgid "SLF4J LOG4J-12 Binding"
msgstr "SLF4J LOG4J-12 バインディング"

#. leap/slf4j/slf4j-log4j12/description
msgid "SLF4J LOG4J-12 Binding."
msgstr "SLF4J LOG4J-12 バインディング。"

#. leap/slf4j/slf4j-manual/summary
msgid "Documents for slf4j"
msgstr "slf4j 向けドキュメンテーション"

#. leap/slf4j/slf4j-manual/description
msgid "Manual for slf4j."
msgstr "slf4j 向けのマニュアルです。"

