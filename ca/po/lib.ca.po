#. tumbleweed/libeconf/libeconf-utils/summary
msgid "Command line interface for libeconf"
msgstr "Interfície de línia d'ordres per al libeconf"

#. tumbleweed/libeconf/libeconf-utils/description
msgid ""
"This package contains tools for handling configuration files in e.g. /usr/"
"etc and /etc."
